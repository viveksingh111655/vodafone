  <property name="address2" targetName="primaryBillToAddressStreetAddress2" include="true"/>
  <property name="address3" targetName="primaryBillToAddressStreetAddress3" include="true"/>
  <property name="addressName" targetName="addressName" include="true"/>
  <property name="country" targetName="primaryBillToCountry" include="true"/>
</item-descriptor>
